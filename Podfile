source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

target 'ScanKit' do
  use_frameworks!
  inhibit_all_warnings!

  pod 'SnapKit'
  pod 'Hue'

  pod 'LookinServer', :configurations => ['Debug']
  pod 'MLeaksFinder', :configurations => ['Debug']
  
end

target 'Sources' do
  use_frameworks!
  inhibit_all_warnings!
  


end
